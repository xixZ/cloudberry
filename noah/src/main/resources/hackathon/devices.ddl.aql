use dataverse hackathon;

drop type DevicesType if exists;
create type DevicesType as closed {
    id: uuid,
    device_id: int64,
    company_id: int32,
    device_type_id: int32,
    device_type: string,
    device_os: int32,
    carrier_name: string?,
    carrier_id: int32?,
    lang: string
}

create dataset devices(DevicesType) primary key id autogenerated;