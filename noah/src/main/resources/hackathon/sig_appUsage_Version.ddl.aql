use dataverse hackathon;
drop dataset sig_app if exists;
drop type SigAppUVType if exists;
create type SigAppUVType as closed{
    id: uuid,
    app_start_time: datetime,
    location: point,
    geo_tag: geoTagType,
    device_id: int64,
    app_id: int64,
    app_usage_type: int8,
    app_name: string,
    app_version_type: string,
    icon: string?,
    category: string,
    run_time: int64
}

create dataset sig_app(SigAppUVType) primary key id autogenerated;