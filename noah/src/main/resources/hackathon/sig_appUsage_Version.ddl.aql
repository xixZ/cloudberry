use dataverse hackathon;
create type SigAppUVType as closed{
    id: uuid,
    time: datetime,
    location: point,
    geo_tag: geoTagType,
    device_id: int64,
    app_id: int64,
    app_version_type: string,
    package_name: string,
    app_name: string,
    version: string,
    app_usage_type: int8,
    icon: string,
    category: string

}

create dataset sig_app(SigAppUVType) primary key id autogenerated;