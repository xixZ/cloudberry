use dataverse hackathon;

create type BatteryType as closed {
	id: uuid,
	time: datetime,
	device_id: int64,
	charge_rate: float,
	drain_rate: float,
	delta_time: float,
	is_battery_draining: int32,
	battery_percentage: float?,
	battery_temperature: float?,
	battery_voltage: float?
}

create dataset battery(BatteryType) primary key id autogenerated;